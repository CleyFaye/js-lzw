{
  "name": "@cley_faye/js-lzw",
  "version": "1.0.0",
  "description": "Pure JavaScript implementation of LZW compression",
  "scripts": {
    "test": "nyc mocha src/tests/**/*.test.ts",
    "build": "grunt clean build",
    "prepack": "npm run test && npm run build",
    "clean": "grunt clean"
  },
  "nyc": {
    "require": [
      "esm",
      "ts-node/register"
    ],
    "extension": [
      ".ts",
      ".js"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "Gruntfile.js",
      "test/**/*"
    ],
    "cache": false,
    "all": true
  },
  "esm": {
    "cache": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CleyFaye/js-lzw.git"
  },
  "keywords": [
    "lzw",
    "compression"
  ],
  "author": "Gabriel Paul \"Cley Faye\" Risterucci",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/CleyFaye/js-lzw/issues"
  },
  "homepage": "https://github.com/CleyFaye/js-lzw#readme",
  "devDependencies": {
    "@cley_faye/eslint-config": "^1.0.6",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "esm": "^3.2.25",
    "grunt": "^1.3.0",
    "grunt-banner": "^0.6.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-ts": "^6.0.0-beta.22",
    "load-grunt-tasks": "^5.1.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
